            "console": "none",
            "cwd": "${workspaceFolder}",
            "env": {
                "SUPERFLORE_GITHUB_TOKEN": ""
            },
            "args": [
            ]
        },
        {
            "name": "superflore-gen-ebuilds",
            "type": "python",
            "request": "launch",
            "program": "${env:HOME}/.local/bin/superflore-gen-ebuilds",
            "stopOnEntry": true,
            "console": "none",
            "cwd": "${workspaceFolder}",
            "env": {
                "SUPERFLORE_GITHUB_TOKEN": ""
