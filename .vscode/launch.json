    "version": "0.2.0",
    "configurations": [
        {
            "name": "superflore-gen-oe-recipes",
            "type": "python",
            "request": "launch",
            "program": "${env:HOME}/.local/bin/superflore-gen-oe-recipes",
            "stopOnEntry": false,
            "console": "none",
            "cwd": "${workspaceFolder}",
            "env": {
                "SUPERFLORE_GITHUB_TOKEN": "",
                "ROSDEP_DEBUG": "true"
            },
            "args": [
                "--dry-run",
                "--ros-distro",
                "crystal",
                "--skip-keys",
                "console_bridge", "libopensplice67", "libopensplice69", "rti-connext-dds-5.3.1", "urdfdom_headers",
            ]
        },
        {
            "name": "superflore-check-ebuilds",
